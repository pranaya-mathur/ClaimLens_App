{
  "annotations": {"list": []},
  "panels": [
    {
      "type": "graph",
      "title": "Request Latency (s)",
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 0},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(claimlens_request_latency_seconds_bucket[1m])) by (le))", "refId": "A"}
      ]
    },
    {
      "type": "graph",
      "title": "Requests per second",
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 0},
      "targets": [
        {"expr": "sum(rate(claimlens_requests_total[1m]))", "refId": "A"}
      ]
    },
    {
      "type": "graph",
      "title": "LLM Calls (success/failure)",
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 6},
      "targets": [
        {"expr": "sum(rate(claimlens_llm_calls_total{result=\"success\"}[5m]))", "refId": "A"},
        {"expr": "sum(rate(claimlens_llm_calls_total{result=\"failure\"}[5m]))", "refId": "B"}
      ]
    },
    {
      "type": "stat",
      "title": "Fallbacks / minute",
      "gridPos": {"h": 3, "w": 6, "x": 12, "y": 6},
      "targets": [{"expr": "sum(rate(claimlens_fallbacks_total[5m]))", "refId": "A"}]
    },
    {
      "type": "stat",
      "title": "Cache hits / minute",
      "gridPos": {"h": 3, "w": 6, "x": 18, "y": 6},
      "targets": [{"expr": "sum(rate(claimlens_cache_hits_total[5m]))", "refId": "A"}]
    },
    {
      "type": "graph",
      "title": "Errors by stage",
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 9},
      "targets": [
        {"expr": "sum(rate(claimlens_errors_total[5m])) by (stage)", "refId": "A"}
      ]
    }
  ],
  "title": "ClaimLens Overview",
  "schemaVersion": 16
}
